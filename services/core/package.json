{
  "name": "@sinny777/microservices-core",
  "version": "1.0.1",
  "description": "core utilities for microservices app",
  "author": "Gurvinder Singh (IBM)",
  "license": "Apache-2.0",
  "files": [
    "README.md",
    "dist/**/*"
  ],
  "keywords": [
    "loopback-extension",
    "loopback",
    "loopback-next",
    "authentication"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./dist": "./dist/index.js",
    "./dist/": "./dist/",
    "./package.json": "./package.json"
  },
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12.0.0",
    "npm": "6.13.0"
  },
  "scripts": {
    "test": "mocha",
    "lint": "eslint .",
    "clean": "lb-clean dist *.tsbuildinfo",
    "build": "npm run clean && lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "setup": "npm link",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "@loopback/authentication": "^4.2.6",
    "@loopback/authentication-passport": "^2.1.6",
    "@loopback/boot": "^2.3.2",
    "@loopback/context": "^3.8.2",
    "@loopback/core": "^2.7.1",
    "@loopback/repository": "^2.6.0",
    "@loopback/rest": "^5.1.0",
    "debug": "^4.1.1",
    "get-keycloak-public-key": "^1.0.3",
    "jsonwebtoken": "^8.5.1",
    "keycloak-admin": "^1.13.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2-client-password": "^0.1.2"
  },
  "devDependencies": {
    "@loopback/boot": "^1.4.4",
    "@loopback/build": "^5.4.2",
    "@loopback/context": "^1.20.2",
    "@loopback/core": "^1.8.5",
    "@loopback/eslint-config": "^8.0.0",
    "@loopback/rest": "^5.1.0",
    "@loopback/testlab": "^3.1.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.13",
    "@types/passport": "^1.0.3",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/passport-http": "^0.3.8",
    "@types/passport-http-bearer": "^1.0.35",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/passport-oauth2-client-password": "^0.1.2",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "lodash": "^4.17.15",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
