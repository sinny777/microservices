{
  "name": "@sinny777/microservices-core",
  "version": "1.0.6",
  "description": "core utilities for microservices app",
  "author": "Gurvinder Singh (IBM)",
  "license": "Apache-2.0",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "keywords": [
    "loopback-extension",
    "loopback",
    "loopback-next",
    "authentication"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12.0.0",
    "npm": "6.13.0"
  },
  "scripts": {
    "test": "mocha",
    "lint": "eslint .",
    "clean": "lb-clean dist *.tsbuildinfo",
    "build": "npm run clean && lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "setup": "npm link",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "@loopback/boot": "^1.5.5",
    "@loopback/context": "^1.22.1",
    "@loopback/core": "^1.10.1",
    "@loopback/rest": "^1.16.3"
  },
  "dependencies": {
    "@loopback/authentication": "^3.2.4",
    "@loopback/authentication-passport": "^0.1.0",
    "@loopback/repository": "^1.13.1",
    "debug": "^4.0.0",
    "get-keycloak-public-key": "^1.0.3",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2-client-password": "^0.1.2"
  },
  "devDependencies": {
    "@loopback/boot": "^1.4.4",
    "@loopback/build": "^2.0.10",
    "@loopback/context": "^1.20.2",
    "@loopback/core": "^1.8.5",
    "@loopback/eslint-config": "^4.0.2",
    "@loopback/rest": "^1.16.3",
    "@loopback/testlab": "^1.8.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/lodash": "^4.14.138",
    "@types/node": "^10.17.17",
    "@types/passport": "^1.0.1",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/passport-http": "^0.3.8",
    "@types/passport-http-bearer": "^1.0.34",
    "@types/passport-jwt": "^3.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/passport-oauth2-client-password": "^0.1.2",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "lodash": "^4.17.14",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.5"
  }
}
